<p id="notice"><%= notice %></p>

<h1>Skills</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Info</th>
      <th>Type</th>
      <th>Type</th>
      <th>Type value</th>
      <th>Positif</th>
      <th>Skillable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skills.each do |skill| %>
      <tr>
        <td><%= skill.title %></td>
        <td><%= skill.info %></td>
        <td><%= skill.type %></td>
        <td><%= skill.type_id %></td>
        <td><%= skill.type_value %></td>
        <td><%= skill.positif %></td>
        <td><%= skill.skillable_id %></td>
        <td><%= link_to 'Show', skill %></td>
        <td><%= link_to 'Edit', edit_skill_path(skill) %></td>
        <td><%= link_to 'Destroy', skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill', new_skill_path %>
